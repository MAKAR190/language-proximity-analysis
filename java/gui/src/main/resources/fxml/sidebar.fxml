<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.app.controller.SidebarController"
      prefWidth="160" spacing="10" alignment="CENTER">

    <Slider fx:id="depthSlider" min="1" max="3" blockIncrement="1"
            majorTickUnit="1" showTickMarks="true" snapToTicks="true"/>

    <ListView fx:id="graphList" VBox.vgrow="ALWAYS"/>
</VBox>
